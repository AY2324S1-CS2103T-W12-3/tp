@startuml
!include ../style.puml
skinparam ArrowFontStyle plain

Participant ":InterviewEditCommandParser" as iecp LOGIC_COLOR
Participant "<<class>>\nArgumentTokenizer" as at LOGIC_COLOR
Participant "<<class>>\nParserUtil" as pu LOGIC_COLOR
Participant ":ArgumentMultimap" as am LOGIC_COLOR
Participant ":editInterviewDescriptor" as eid LOGIC_COLOR
Participant ":InterviewEditCommand" as iec LOGIC_COLOR

[-> iecp ++: parse(args)
iecp -> at ++: tokenize()
at --> iecp --: argMultimap

iecp -> pu ++: parseIndex()
pu --> iecp --: interviewIndex

iecp -> pu ++: parseIndex()
pu --> iecp --: jobIndex

iecp -> am ++: verifyNoDuplicatePrefixesFor()
deactivate am

create eid
iecp -> eid ++: EditInterviewDescriptor()
eid --> iecp --: editInterviewDescriptor

create iec
iecp -> iec ++: InterviewEditCommand(jobIndex, interviewIndex, editInterviewDescriptor)
iec --> iecp --: result
iecp -->[ --: result
@enduml
