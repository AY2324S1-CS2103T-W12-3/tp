@startuml
!include ../style.puml
skinparam ArrowFontStyle plain

box Logic #skyblue
Participant ":InterviewAddCommand" as iac LOGIC_COLOR
Participant ":CommandResult" as cr LOGIC_COLOR
end box

box Model #pink
Participant "jobToAddInterview:Job" as j MODEL_COLOR
end box

[-> iac ++: execute()
iac -> j ++: getJob()
j --> iac--: jobToAddInterview

iac -> j ++:addInterview()
deactivate j
create cr
iac -> cr ++: CommandResult()
cr -->iac --: result
iac -->[ --: result

@enduml
